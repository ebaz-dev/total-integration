{
  "name": "total-integration",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev --poll src/index.ts",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest --ci --detectOpenHandles --forceExit",
    "clean-common:windows": "cd ..\\common && npm run clean-src:windows && cd ..\\service",
    "pub:windows": "npm run clean-common:windows && xcopy /E /I /Y src\\shared\\* ..\\common\\src && cd ..\\common && npm run pub:windows",
    "clean-common": "cd ./../common && npm run clean-src && cd ../service",
    "pub": "npm run clean-common && cp -rf src/shared/* ../common/src && cd ../common && npm run pub",
    "lint": "tsc --noEmit",
    "build": "tsc -p ./tsconfig.json"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@ebazdev/auth": "^1.0.20",
    "@ebazdev/core": "^1.0.30",
    "@ebazdev/customer": "^1.0.20",
    "@ebazdev/order": "^1.0.34",
    "@ebazdev/product": "^1.0.75",
    "@types/cookie-session": "^2.0.49",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/node-cron": "^3.0.11",
    "axios": "^1.7.7",
    "cookie-session": "^2.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-validator": "^7.2.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.5.3",
    "mongoose-update-if-current": "^1.4.0",
    "node-cron": "^3.0.3",
    "node-nats-streaming": "^0.3.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.0.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.4"
  }
